{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node",
        "functions": {
          "default": {
            "maxDuration": 30
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ]
  }
  